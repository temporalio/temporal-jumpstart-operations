{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "grpc-route": {
        "ClusterId": "temporal-cluster",
        "Match": {
          "Path": "{**catch-all}"
        },
        "Transforms": [
          { "RequestHeadersCopy": "true" },
          { "ResponseHeadersCopy": "true" }
        ]
      }
    },
    "Clusters": {
      "temporal-cluster": {
        "HttpRequest": {
          "Version": "2.0",
          "VersionPolicy": "RequestVersionExact"
        },
        "HttpClient": {
          "DangerousAcceptAnyServerCertificate": true,
          "MaxConnectionsPerServer": 1024
        },
        "Destinations": {
          "temporal-server": {
            "Address": "http://localhost:7233"
          }
        },
        "HealthCheck": {
          "Active": {
            "Enabled": false
          },
          "Passive": {
            "Enabled": false
          }
        }
      }
    }
  },
  "Encryption": {
    "DefaultKeyId": "default-key-2024",
    "KeyIdPrefix": "temporal_payload_"
  },
  "Protobuf": {
    "DescriptorFiles": {
      "TemporalApi": "../../../generated/temporal-api.binpb"
    }
  }
}